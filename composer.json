{
    "name": "treyworks-search/wordpress-plugin",
    "description": "A WordPress plugin with AI-powered search capabilities",
    "type": "wordpress-plugin",
    "version": "1.1.0",
    "require": {
        "php": ">=7.4",
        "openai-php/client": "^0.7.3",
        "php-http/guzzle7-adapter": "^1.0",
        "guzzlehttp/guzzle": "^7.5",
        "google-gemini-php/client": "^1.0",
        "php-http/discovery": "^1.19",
        "php-http/client-common": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "QuickSearchSummarizer\\": "includes/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
